<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="我自己的自动化测试套件">
    <test name="imooc">
        <classes>
            <!--引入配置文件类-->
            <class name="com.imooc.suite.SuiteConfig"></class>
            <class name="com.imooc.suite.LoginTest"></class>
        </classes>
    </test>
    <test name="imooc pay">
        <classes>
            <!--引入配置文件类-->
            <class name="com.imooc.suite.SuiteConfig"></class>
            <class name="com.imooc.suite.PayTest"></class>
        </classes>
    </test>
    <test name="ignore test">
        <classes>
            <class name="com.imooc.IgnoreTest"></class>
        </classes>
    </test>
</suite>